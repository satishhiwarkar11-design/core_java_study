package com.cjc;

public class parameterized {
int rollno;
String name;
	
	public parameterized(int rollno,String name)
	{
	this.rollno=rollno;
	this.name=name;
	}
	public void display()
	{
		
		System.out.println("rollno:="+rollno+"\n"+"Name:="+name+"");
	}
	public static void main(String[] args) {
		parameterized p=new parameterized(10,"satish");
	}
}
